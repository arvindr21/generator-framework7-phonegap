.page(data-page="swiper-gallery")
  .navbar
    .navbar-inner
      .left
        a(href="swiper.html").back.link.icon-only
          i.icon.icon-back
      .center Two Way Control Gallery
      .right
        a(href="#").open-panel.link.icon-only
          i.icon.icon-bars
  .page-content
    .swiper-container.ks-swiper-gallery-top
      .swiper-wrapper
        - for (var i = 1; i < 10; i++)
          .swiper-slide(style="background-image:url(http://lorempixel.com/800/800/nature/#{i})")
      .swiper-button-next.color-white
      .swiper-button-prev.color-white
    .swiper-container.ks-swiper-gallery-thumbs
      .swiper-wrapper
        - for (var i = 1; i < 10; i++)
          .swiper-slide
            .swiper-slide-pic(style="background-image:url(http://lorempixel.com/800/800/nature/#{i})")