.page(data-page="searchbar")
  .navbar
    .navbar-inner
      .left
        a(href="index.html").back.link.icon-only
          i.icon.icon-back
      .center Search Bar
      .right
        a(href="#").link.open-panel.icon-only
          i.icon.icon-bars
  form.searchbar.searchbar-init(data-search-list=".search-here", data-search-in=".item-title")
    .searchbar-input
      input(type="search", placeholder="Search")
      a(href="#").searchbar-clear
  .searchbar-overlay
